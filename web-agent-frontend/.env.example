# Backend API Configuration
# The backend API runs on port 8000 by default
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000

# OnKernal Browser Configuration
# The OnKernal Browser UI runs on port 8080 by default
REACT_APP_BROWSER_URL=http://localhost:8080

# API Prefix (matches backend configuration)
REACT_APP_API_PREFIX=/api/v1
